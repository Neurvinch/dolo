# Kestra Configuration
KESTRA_API_URL=http://localhost:8080
KESTRA_WEBSOCKET_URL=ws://localhost:8080

# Vercel Configuration
VERCEL_URL=http://localhost:3000
NEXT_PUBLIC_API_URL=http://localhost:3000/api

# Oumi Configuration
OUMI_MODEL_PATH=./oumi/models
OUMI_DATASET_PATH=./oumi/training/dataset

# API Keys (DO NOT COMMIT REAL VALUES)
# Add your API keys in .env file (not tracked by git)
API_TOKEN=your_api_token_here
